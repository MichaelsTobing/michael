<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="cron_cancelorder" instance="Icube\PaymentReminder\Cron\Job" method="execute">
        	<schedule>*/15 * * * *</schedule>
        </job>
        <job name="cron_syncdataorder" instance="Icube\PaymentReminder\Cron\Job" method="sync">
        	<schedule>0 */2 * * *</schedule>
        </job>
    </group>
</config>