<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Sales\Block\Order\Info" name="progress.bar" template="Magento_Sales::progress-bar.phtml" before="-"/>
            <block class="Magento\Framework\View\Element\Template" name="back-order" template="Magento_Sales::back-order.phtml" after="sales.order.info"/>
        </referenceContainer>
        <move element="sales.order.info" destination="content" after="progress.bar"/>
    </body>
</page>
